@{
    ViewBag.Title = "Home Page";
}

    <body>
        <div id="bodyDiv">
            Welcome To 
            <br />
            KrugDevelopment
        </div>

        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.3.1/css/bootstrap.min.css">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0-11/css/all.min.css">
        <link rel="stylesheet" href="style.css">

        <div id="my-carousel" class="carousel slide" data-interval="2000" data-ride="carousel" data-pause="hover" data-wrap="true">
            <ol class="carousel-indicators">
                <!--Starting slide.-->
                <li class="active" data-target="#my-carousel" data-slide-to="0" aria-current="location"></li>
                <!--Next slides. Need to add new data-target for each additional slide.-->
                <li data-target="#my-carousel" data-slide-to="1"></li>
                <li data-target="#my-carousel" data-slide-to="2"></li>
            </ol>
            <!--Starting slide.-->
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img class="d-block w-100" src="~/Content/emile-perron-xrVDYZRGdw4-unsplash.jpg" alt="Photo by Émile Perron on Unsplash">
                    <div class="carousel-caption d-none d-md-block">
                        <h5>Application Development</h5>
                        <p>Associates Degree: Computer Science and Engineering</p>
                    </div>
                </div>
                <!--Next slides. Need to add new carousel-item for each additional slide.-->
                <div class="carousel-item">
                    <img class="d-block w-100" src="~/Content/pakata-goh-EJMTKCZ00I0-unsplash.jpg" alt="Photo by Pakata Goh on Unsplash">
                    <div class="carousel-caption d-none d-md-block">
                        <h5>Game Development</h5>
                        <p>Associates Degree: Game Design and Development</p>
                    </div>
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="~/Content/cabinet-data-data-center-325229.jpg" alt="Photo by Manuel Geissinger on Pexels">
                    <div class="carousel-caption d-none d-md-block">
                        <h5>Database Management</h5>
                        <p>Associates Degree: Programming and Database</p>
                    </div>
                </div>
            </div>
            <a class="carousel-control-prev" href="#my-carousel" data-slide="prev" role="button">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#my-carousel" data-slide="next" role="button">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </body>

